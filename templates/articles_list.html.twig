{# templates/default/index.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}
  <h1>Articles</h1>
  <table class="table">
    <tr>
      <th>Id</th>
      <th>State</th>
    </tr>
    {% for article in articles %}
      <tr>
        <td>{{ article.id }}</td>
        <td>{{ article.currentPlace|join }}</td>
      </tr>
    {% endfor %}
  </table>
{% endblock %}